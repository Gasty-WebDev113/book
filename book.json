{
  "plugins": [
    "-lunr",
    "-search",
    "disqus",
    "expo-snack",
    "ga",
    "edit-link",
    "github-buttons"
  ],
  "pdf": {
    "paperSize": "a4"
  },
  "pluginsConfig": {
    "disqus": {
      "shortName": "reactnative-guide"
    },
    "edit-link": {
      "base": "https://github.com/react-made-native-easy/book/blob/master",
      "label": "Edit"
    },
    "ga": {
      "token": "UA-111146084-1"
    },
    "github-buttons": {
     "buttons": [{
       "user": "react-made-native-easy",
       "repo": "book",
       "type": "star",
       "size": "small"
     }]
   }
  }
}
